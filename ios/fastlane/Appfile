app_identifier(ENV['APP_IDENTIFIER'] || 'com.example.aurora') # Bundle ID
apple_id(ENV['APPLE_ID'] || 'your-apple-id@example.com')      # Apple ID used for App Store Connect

# Apple Developer Program team (Certificates/Profiles)
team_id(ENV['DEVELOPER_TEAM_ID'] || '')

# App Store Connect team
itc_team_id(ENV['APP_STORE_CONNECT_TEAM_ID'] || '')

